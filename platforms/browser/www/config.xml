<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="0.0.2" id="xdk.intel.autosoft" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Easier service</name>
    <description>
      For technicians and assessors.
  </description>
    <author email="idvazquezrivera@gmail.com" href="http://ivanvazquez.top">
     Ivan Vazquez
  </author>
    <content src="index.html" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <access origin="*" />
    <config-file mode="merge" parent="/manifest/application" platform="android">
        <activity android:launchMode="singleTop" />
    </config-file>
    <preference name="android-build-tool" value="gradle" />
    <preference name="xwalkMultipleApk" value="false" />
    <preference name="SplashScreenDelay" value="5000" />
    <preference name="xwalkCommandLine" value="--disable-pull-to-refresh-effect --ignore-gpu-blacklist" />
    <preference name="SplashMaintainAspectRatio" value="false" />
</widget>
